{
  "name": "ocpp-ws-io-monorepo",
  "version": "2.1.4",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "test": "turbo run test",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "commit": "cz",
    "changeset": "changeset",
    "format": "biome format --write .",
    "check": "biome check --write .",
    "prepublishOnly": "npm run build",
    "prepare": "npm -e \"if (!process.env.CI) process.exit(1)\" || husky",
    "commitlint": "commitlint --edit",
    "version": "changeset version",
    "release": "npm run build && changeset publish"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "author": "Rohit Tiwari <rohit@rohittiwari.me>",
  "homepage": "https://ocpp-ws-io.rohittiwari.me",
  "bugs": {
    "url": "https://github.com/rohittiwari-dev/ocpp-ws-io/issues"
  },
  "keywords": [
    "ocpp",
    "ocpp-j",
    "ocpp1.6",
    "ocpp1.6j",
    "ocpp2.0.1",
    "ocpp2.1",
    "websocket",
    "rpc",
    "json-rpc",
    "ev-charging",
    "evse",
    "csms",
    "cpo",
    "emsp",
    "charge-point",
    "charging-station",
    "charge-point-simulator",
    "smart-charging",
    "electric-vehicle",
    "e-mobility",
    "iso15118",
    "type-safe",
    "typescript",
    "nodejs",
    "redis",
    "clustering",
    "pubsub",
    "browser",
    "react-native"
  ],
  "devDependencies": {
    "@biomejs/biome": "^2.4.2",
    "@changesets/cli": "^2.29.8",
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "commitizen": "^4.3.1",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^9.1.7",
    "turbo": "^2.8.10"
  },
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "packageManager": "^npm@10.2.4",
  "dependencies": {
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-presence": "^1.1.5",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "class-variance-authority": "^0.7.1",
    "fumadocs-core": "^16.6.3",
    "fumadocs-ui": "^16.6.3",
    "lucide-react": "^0.570.0",
    "next-themes": "^0.4.6",
    "react": "^19.2.4",
    "scroll-into-view-if-needed": "^3.1.0",
    "tailwind-merge": "^3.4.1"
  }
}
